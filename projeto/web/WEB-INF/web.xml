<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         version="5.0">

    <servlet>
        <servlet-name>RefeicaoServlet</servlet-name>
        <servlet-class>servlets.RefeicaoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RefeicaoServlet</servlet-name>
        <url-pattern>/RefeicaoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PedidoServlet</servlet-name>
        <servlet-class>servlets.PedidoServlet</servlet-class>
    </servlet>
